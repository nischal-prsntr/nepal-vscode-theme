    "titleBar.border": "#2c3e50"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6c7b95",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#f39c12"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#dc143c"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#e74c3c"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#dc143c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#2c3e50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#3498db",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f39c12",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#3498db",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#dc143c"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#f39c12"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#9b59b6"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#e74c3c"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#d35400"
      }
    },
    {
      "name": "JSON String",
      "scope": [
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#f39c12"
      }
    },
    {
      "name": "JSON Key",
      "scope": [
        "meta.structure.dictionary.key.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#3498db"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#dc143c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#f39c12",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#dc143c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#3498db",
        "fontStyle": "underline"
      }
    }
  ]
}
